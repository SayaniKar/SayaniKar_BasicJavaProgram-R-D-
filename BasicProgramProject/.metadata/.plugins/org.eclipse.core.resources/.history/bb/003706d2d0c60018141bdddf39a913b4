public class BankAccount {
	//Encapsulation Concept is added
	int accountNo;
	String accountHolderName;
	double accountBalance;
	public BankAccount() {
		//super();
		accountNo=1;
		accountHolderName="unknown";
		accountBalance=0;
		
	}
	

}
